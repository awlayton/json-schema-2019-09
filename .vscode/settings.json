{
  "search.exclude": {
    ".dccache": true,
    ".history": true,
    ".nvimlog": true,
    "node_modules": true,
    "package.lock": true,
    "*.log": true,
    ".DS_Store": true,
    "dist": true,
    ".test": true,
    "*.tsbuildinfo": true,
    ".actrc": true,
    "act": true,
    ".yarn": true,
    ".pnp.*": true,
    ".vim": true,
    "**/.yarn": true,
    "coverage": true,
    "tags.*": true,
    "**/.pnp.*": true
  },
  "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "eslint.nodePath": ".yarn/sdks",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/tags": true,
    ".dccache": true,
    ".history": true,
    ".nvimlog": true,
    "node_modules": true,
    "package.lock": true,
    "*.log": true,
    ".DS_Store": true,
    "dist": true,
    ".test": true,
    "*.tsbuildinfo": true,
    ".actrc": true,
    "act": true,
    ".yarn": true,
    ".pnp.*": true,
    ".vim": true,
    "coverage": true,
    "tags.*": true
  },
  "eslint.packageManager": "yarn",
  "licenser.author": "Qlever LLC",
  "licenser.license": "AL2",
  "cSpell.words": [
    "Buildx",
    "codeql",
    "lcov",
    "pino",
    "Snyk",
    "sonarjs",
    "trellisfw"
  ],
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.useESLintClass": true,
  "explorerExclude.backup": null,
  "npm.packageManager": "yarn",
  "debug.javascript.autoAttachFilter": "smart",
  "debug.javascript.breakOnConditionalError": true,
  "debug.javascript.defaultRuntimeExecutable": {
    "node": "yarn",
    "pwa-node": "yarn"
  },
  "debug.javascript.autoAttachSmartPattern": [
    "${workspaceFolder}/**",
    "!**/node_modules/**",
    "**/$KNOWN_TOOLS$/**",
    "!**/yarn-*",
    "!**/ava-*/**/cli.mjs"
  ]
}
